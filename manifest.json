{
    "manifest_version": 3,
    "name": "__MSG_extension_name__",
    "version": "0.1",
    "default_locale": "en",
    "description": "__MSG_extension_description__",
    "permissions": ["storage", "scripting"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "dart.png",
        "24": "dart.png",
        "48": "dart.png",
        "128": "dart.png"
      }
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ]
  }
  